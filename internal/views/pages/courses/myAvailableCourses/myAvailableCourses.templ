package myAvailableCourses

import (
	"fmt"
	"github.com/jonahhess/ds/internal/types"
)

templ MyAvailableCourses(courses []types.Item) {
	<h1>All My Courses</h1>
	<ul>
		for _, v := range courses {
			<li>
				<a href={ fmt.Sprintf("/catalog/courses/%d", v.ID) }>{ v.Text }</a>
			</li>
		}
	</ul>
}
